<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module rename-to="showcase">

  <inherits name='com.google.gwt.user.theme.standard.Standard'/>

	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.logging.Logging"/>
	<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />
	<set-property name="gwt.logging.consoleHandler" value="ENABLED" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
	<inherits name='de.kurka.phonegap.PhoneGap' />
	
	<set-property name="user.agent" value="safari" />
	
	<inherits name="com.google.gwt.core.Core" />
		<set-property name="compiler.emulatedStack" value="true"/>
	<set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="true"/>
	<set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true"/>

	
	
	

	<entry-point class="de.kurka.phonegap.showcase.client.ShowCaseEntryPoint" />
	
	
	
	<source path="client"/>
</module>