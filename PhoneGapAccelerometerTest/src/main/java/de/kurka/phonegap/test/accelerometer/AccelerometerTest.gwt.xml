<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='acceltest'>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Other module inherits                                      -->
	<inherits name='de.kurka.phonegap.PhoneGap' />



	<!-- only compile for webkit browser i.e. mobile safari-->
	<set-property name="user.agent" value="safari" />

	<entry-point class="de.kurka.phonegap.test.accelerometer.client.AccelerometerEntryPoint" />

	<!-- Specify the paths for translatable code                    -->
	<source path='client' />

</module>
